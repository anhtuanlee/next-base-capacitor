{
  "name": "next-capacitor-mono",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build:web": "npm run -w apps/web build",
    "build:mobile": "npm run -w apps/mobile build",
    "dev:mobile": "npm run -w apps/mobile dev",
    "mobile:sync": "npm run build:mobile && npx cap sync",
    "mobile:sync:android": "npm run build:mobile && npx cap sync android",
    "mobile:open:android": "npm run mobile:sync:android && npx cap open android",
    "mobile:sync:ios": "npm run build:mobile && npx cap sync ios",
    "android": "npm run mobile:sync:android && npx cap open android",
    "ios": "npm run mobile:sync:ios && npx cap open ios"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.3",
    "@tanstack/react-query": "^5.58.0",
    "next": "15.5.3",
    "next-auth": "^4.24.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zustand": "^4.5.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
